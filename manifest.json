{
  "name": "Delicious",
  "version": "0.7",
  "manifest_version": 2,
  "description": "Chrome extension that adds links and manages bookmarks from your Delicious account.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "https://api.del.icio.us/",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128":"icon-128.png"
  },
  "web_accessible_resources": [ "popup.html" ]
}
