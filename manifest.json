{

 "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "description": "Chrome extension with context menu that adds links and manages bookmarks from your Delicious account.",
  "icons": {
    "16": "images/icon-16.png",
    "128":"images/icon-128.png"
  },
  "manifest_version": 2,
  "name": "@Delicious",
  "omnibox": { "keyword" : "@delicious" },
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "https://api.del.icio.us/",
    "tabs"
  ],
  "version": "1.2",
  "web_accessible_resources": [ "popup.html" ]

}
