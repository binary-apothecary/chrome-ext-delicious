{

 "background": {
    "scripts": [
      "/assets/javascripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/assets/images/icon.png",
    "default_popup": "popup.html",
    "default_title": "@Delicious"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "MacCtrl+Shift+D"
      }
    }
  },
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": [
      "/vendor/javascripts/jquery-2.0.0.min.js",
      "/assets/javascripts/tab.js"
    ],
    "css": [ "/assets/stylesheets/tab.css" ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Feature rich Chrome extension with the ability to add, edit, filter, and remove bookmarks from your Delicious account.",
  "incognito": "spanning",
  "icons": {
    "16": "/assets/images/icon-16.png",
    "128":"/assets/images/icon-128.png"
  },
  "manifest_version": 2,
  "name": "@Delicious",
  "omnibox": {
    "keyword" : "@del"
  },
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "https://api.del.icio.us/",
    "tabs"
  ],
  "version": "2.9.3",
  "web_accessible_resources": [ "/popup.html" ]

}
