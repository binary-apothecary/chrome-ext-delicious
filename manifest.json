{
  "name": "Delicious",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Chrome extension that adds links and manages bookmarks from your Delicious account.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://api.del.icio.us/"
  ],
  "icons": {
    "128":"icon-128.png"
  }
}
